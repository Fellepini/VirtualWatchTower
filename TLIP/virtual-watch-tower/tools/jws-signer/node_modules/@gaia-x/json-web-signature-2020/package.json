{
  "name": "@gaia-x/json-web-signature-2020",
  "version": "2.1.0",
  "description": "A lightweight JsonWebSignature2020 signing and verification Typescript library by Gaia-X AISBL",
  "homepage": "https://gitlab.com/gaia-x/lab/json-web-signature-2020",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:gaia-x/lab/json-web-signature-2020.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepare": "husky",
    "compile": "tsc --noEmit",
